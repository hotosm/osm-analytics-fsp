{
  "version": 8,
  "sources": {
    "osm-mobilemoney-raw": {
      "type": "vector",
      "tiles": [
        "{{server}}/mobilemoney/{z}/{x}/{y}.pbf"
      ],
      "minzoom": 13,
      "maxzoom": 14
    },
    "osm-mobilemoney-aggregated": {
      "type": "vector",
      "tiles": [
        "{{server}}/mobilemoney/{z}/{x}/{y}.pbf"
      ],
      "minzoom": 0,
      "maxzoom": 12
    }
  },
  "layers": [
    {
      "id": "mobilemoney-raw",
      "type": "fill",
      "source": "osm-mobilemoney-raw",
      "source-layer": "osm",
      "paint": {
        "fill-color": "#FDB863",
        "fill-opacity": 1,
        "fill-outline-color": "#E08214"
      },
      "filter": [
        "==",
        "$type",
        "Polygon"
      ]
    },
    {
      "id": "mobilemoney-raw-road",
      "type": "line",
      "source": "osm-mobilemoney-raw",
      "source-layer": "osm",
      "paint": {
        "line-width": 1,
        "line-color": "#9e9ac8",
        "line-opacity": 1
      },
      "filter": [
        "==",
        "$type",
        "LineString"
      ]
    },
    {
      "id": "mobilemoney-raw-mm",
      "type": "circle",
      "source": "osm-mobilemoney-raw",
      "source-layer": "osm",
      "paint": {
        "circle-color": "#FF0000",
        "circle-opacity": 1
      },
      "filter": [
        "==",
        "$type",
        "Point"
      ]
    },
    {
      "id": "mobilemoney-raw-highlight",
      "type": "fill",
      "source": "osm-mobilemoney-raw",
      "source-layer": "osm",
      "paint": {
        "fill-color": "#5CBAD8",
        "fill-opacity": 1
      },
      "filter": [
        "==",
        "_timestamp",
        -1
      ]
    },



    {
      "id": "mobilemoney-aggregated-0",
      "type": "fill",
      "source": "osm-mobilemoney-aggregated",
      "source-layer": "osm",
      "maxzoom": 12.01,
      "paint": {
        "fill-color": "#ffe4b2"
      },
      "filter": [
        "all",
        [
          ">=",
          "_noOfMMAgents",
          1
        ],
        [
          "<",
          "_noOfMMAgents",
          25
        ]
      ]
    },
    {
      "id": "mobilemoney-aggregated-1",
      "type": "fill",
      "source": "osm-mobilemoney-aggregated",
      "source-layer": "osm",
      "maxzoom": 12.01,
      "paint": {
        "fill-color": "#ffdb99",
        "fill-antialias": false
      },
      "filter": [
        "all",
        [
          ">=",
          "_noOfMMAgents",
          25
        ],
        [
          "<",
          "_noOfMMAgents",
          50
        ]
      ]
    },
    {
